local function updateSpeed()
    while true do
        wait(0.1)
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = SliderValue
        end
    end
end

spawn(updateSpeed)


